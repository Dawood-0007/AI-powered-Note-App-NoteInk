{
  "expo": {
    "name": "NoteInk",
    "slug": "noteink",
    "owner": "abdullah7007",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/NoteInk.png",
    "scheme": "noteink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/NoteInk.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.abdullah7007.noteink"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/NoteInk.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/NoteInk.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "891b6adb-9932-4d3c-8a1d-ecc77c22d15e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/891b6adb-9932-4d3c-8a1d-ecc77c22d15e"
    }
  }
}
